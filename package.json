		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.14",
		"@types/bcrypt": "^5.0.2",
		"@types/eslint": "^9.6.0",
		"@types/jsonwebtoken": "^9.0.7",
		"@types/node": "^22.8.5",
		"autoprefixer": "^10.4.20",
