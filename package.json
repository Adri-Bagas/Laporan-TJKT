		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^3.4.9",
		"ts-node": "^10.9.2",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0",
		"vite": "^5.0.3"
	},
	"dependencies": {
		"bcrypt": "^5.1.1",
		"dotenv": "^16.4.5",
